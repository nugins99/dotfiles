set editing-mode vi 
set keymap vi-command

$if mode=vi
    set keymap vi-command
    "jk": vi-insertion-mode
    set keymap vi-insert
    "jk": vi-movement-mode
$endif

"\e\C-m": '\C-a "$(\C-e)"\C-a'

"\ez": 'cd -\015'
"\ez": 'iecho "hello'


Control-w: '\e^i $(\e$i)\e^i'


